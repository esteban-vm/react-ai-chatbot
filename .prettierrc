{
  "semi": false,
  "tabWidth": 2,
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxSingleQuote": true,
  "endOfLine": "lf",
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss",
    "prettier-plugin-tailwind-styled-components",
    "prettier-plugin-classnames",
    "prettier-plugin-merge"
  ],
  "tailwindFunctions": ["clsx", "cn", "tw"],
  "tailwindStylesheet": "./src/index.css",
  "importOrder": [
    "<TYPES>^(node:)",
    "<TYPES>",
    "<TYPES>^@(/.*)$",
    "<TYPES>^[./]",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^@(/.*)$",
    "^[./]"
  ],
  "importOrderCaseSensitive": true
}
